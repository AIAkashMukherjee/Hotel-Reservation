pipeline {
    agent any
    stages {
        stage('Cloning Github repo to Jenkins') {
            steps {
                checkout scmGit(
                    branches: [[name: '*/main']],
                    userRemoteConfigs: [[
                        credentialsId: 'github-token',
                        url: 'https://github.com/AIAkashMukherjee/Hotel-Reservation.git'
                    ]]
                )
            }
        }
    }
}
